{
  "name": "@vleytman/af",
  "version": "1.0.13",
  "description": "Small animation framework to create animations with HTML attributes",
  "main": "dist/af-gsap.umd.js",
  "module": "dist/af-gsap.esm.js",
  "exports": {
    ".": {
      "import": "./dist/af-gsap.esm.js",
      "require": "./dist/af-gsap.umd.js"
    },
    "./core": {
      "import": "./dist/af-core.esm.js",
      "require": "./dist/af-core.umd.js"
    },
    "./gsap": {
      "import": "./dist/af-gsap.esm.js",
      "require": "./dist/af-gsap.umd.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c rollup.config.mjs",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "animation",
    "gsap",
    "animejs"
  ],
  "author": "Vasiliy Leytman",
  "license": "MIT",
  "dependencies": {},
  "peerDependencies": {
    "gsap": "^3.0.0",
    "animejs": "^3.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^23.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-terser": "^0.3.0",
    "rollup": "^3.0.0"
  }
}